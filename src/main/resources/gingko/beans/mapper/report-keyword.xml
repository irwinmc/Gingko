<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gingko.app.persist.mapper.ReportKeywordMapper">

	<resultMap id="reportKeywordMap" type="org.gingko.app.persist.domain.ReportKeyword">
		<result property="id" column="id"/>
		<result property="type" column="type"/>
		<result property="ruleId" column="rule_id"/>
		<result property="keyword" column="keyword"/>
	</resultMap>

	<select id="select" resultMap="reportKeywordMap">
		select * from gingko.gk_report_keyword
	</select>

	<delete id="deleteById" parameterType="int">
		delete from gingko.gk_report_keyword where "id"=#{id}
	</delete>

</mapper>