<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gingko.app.persist.mapper.ReportKeywordMapper">

	<select id="select" resultType="org.gingko.app.persist.domain.ReportKeyword">
		select * from gk_report_keyword
	</select>

	<delete id="deleteById" parameterType="int">
		delete from gk_report_keyword where id=#{id}
	</delete>

</mapper>