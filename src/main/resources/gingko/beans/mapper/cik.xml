<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gingko.app.persist.mapper.usk.CikMapper">

	<resultMap id="cikMap" type="org.gingko.app.persist.domain.usk.Cik">
        <id column="id" property="id"/>
		<result column="cik" property="cik"/>
		<result column="code" property="code"/>
	</resultMap>

    <select id="selectAll" resultMap="cikMap">
        select * from gingko.usk_cik
    </select>

    <select id="selectCodeByCik" resultType="String">
        select "code" from gingko.usk_cik where "cik"=#{cik}
    </select>

    <insert id="insertList" parameterType="java.util.List">
        insert into gingko.usk_cik (
            "cik", "code"
        ) values
        <foreach collection="list" item="item" index="index" open="(" separator="),(" close=")">
            #{item.cik}, #{item.code}
        </foreach>
    </insert>

    <delete id="delete">
		delete from gingko.usk_cik where "cik"=#{cik}
	</delete>

</mapper>