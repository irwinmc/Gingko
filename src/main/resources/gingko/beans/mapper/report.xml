<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gingko.app.persist.mapper.ReportMapper">

	<resultMap id="reportMap" type="org.gingko.app.persist.domain.Report">
		<result property="type" column="type"/>
		<result property="name" column="name"/>
	</resultMap>

	<select id="select" resultMap="reportMap">
		select * from gingko.gk_report
	</select>

</mapper>